{
  "name": "kyokan-plasma-client",
  "version": "0.0.3",
  "description": "A JavaScript client library for Kyokan's Plasma implementation.",
  "main": "./lib/Plasma.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --project . && npm run copy-protofile",
    "clean": "rm -rf ./dist",
    "copy-protofile": "mkdir -p ./lib/rpc/pb && cp ../rpc/proto/root.proto ./lib/rpc/pb",
    "protogen-web": "protoc -I=../rpc/proto root.proto --grpc-web_out=import_style=commonjs+dts,mode=grpcwebtext:./lib/rpc/pb",
    "build-docs": "typedoc --excludeNotExported --excludePrivate --mode file --out ./docs src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyokan/plasma.git"
  },
  "keywords": [
    "plasma",
    "ethereum",
    "scalability"
  ],
  "author": "Kyokan Group Incorporated",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyokan/plasma/issues"
  },
  "homepage": "https://github.com/kyokan/plasma#readme",
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "mocha": "5.2.0",
    "typedoc": "0.14.2",
    "typescript": "3.2.4"
  },
  "dependencies": {
    "@babel/runtime": "7.3.1",
    "@grpc/proto-loader": "0.4.0",
    "@types/bn.js": "4.11.4",
    "@types/ethereumjs-util": "5.2.0",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.18",
    "@types/rimraf": "2.0.2",
    "@types/web3": "1.0.18",
    "bn.js": "4.11.8",
    "ethereumjs-util": "6.0.0",
    "grpc": "1.18.0",
    "grpc-web": "1.0.3",
    "int64-buffer": "0.99.1007",
    "rimraf": "2.6.3",
    "truffle": "5.0.2",
    "web3": "1.0.0-beta.37"
  }
}
